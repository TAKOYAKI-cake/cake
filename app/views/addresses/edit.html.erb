<h3>配送先編集</h3>
    <%= form_for @address do |f| %>
     <div>
  	   <label>郵便番号</label>
  	   <%= f.text_field :post_code %>
     </div>
     <div>
  	   <label>住所</label>
  	   <%= f.text_field :shipping_address %>
     </div>
     <div>
       <label>宛名</label>
       <%= f.text_field :shipping_name %>
     </div>
     <div>
  	   <%= f.submit "編集する" %>
     </div>
    <% end %>

<h1>Addresses#edit</h1>
<p>Find me in app/views/addresses/edit.html.erb</p>
