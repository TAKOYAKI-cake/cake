<h3>登録情報編集</h3>
<%= link_to congfirm_path do %>
退会
<% end %>
<%= form_for(@customer) do |f| %>
	<table>
	  <tr>
	    <th>氏名</th>
	    <th>
	      <div class="field">
	        <%= f.text_field :family_name, autofocus: true, autocomplete: "family_name" %>
	        <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
	      </div>
	    </th>
	  </tr>

	  <tr>
	    <th>カナ</th>
	    <th>
	      <div class="field">
	        <%= f.text_field :family_name_kana, autofocus: true, autocomplete: "family_name_kana" %>
	        <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "last_name_kana" %>
	      </div>
	    </th>
	  </tr>

	  <tr>
	    <th>郵便番号</th>
	    <th>
	      <div class="field">
	        <%= f.text_field :post_code, autofocus: true, autocomplete: "post_code" %>
	      </div>
	    </th>
	  </tr>

	  <tr>
	    <th>住所</th>
	    <th>
	      <div class="field">
	        <%= f.text_field :address, autofocus: true, autocomplete: "address" %>
	      </div>
	    </th>
	  </tr>

	  <tr>
	    <th>電話番号</th>
	    <th>
	      <div class="field">
	        <%= f.text_field :telephone_number, autofocus: true, autocomplete: "telephone_number" %>
	      </div>
	    </th>
	  </tr>

	  <tr>
	    <th>メールアドレス</th>
	    <th>
	      <div class="field">
	        <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
	      </div>
	    </th>
	  </tr>
	</table>

	<div class="actions">
		<%= f.submit "編集内容を保存する" %>
	</div>
<% end %>